#version 330 core
out vec4 FragColor; 

in vec3 ourColor; 
in vec2 TexCoord;

uniform sampler2D texture1;
uniform sampler2D texture2;
	
void main()	
{ 
	vec2 newTexCoord = TexCoord;
	//newTexCoord.x *= 2;
	//newTexCoord.y *= 2;
	FragColor = mix(texture(texture1, TexCoord), texture(texture2, newTexCoord), 0.2);
} 